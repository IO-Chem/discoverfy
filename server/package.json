{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_ENV=development npm run watch:dev",
    "prod": "NODE_ENV=production npm-run-all build server",
    "server": "node ./dist/bin/www",
    "transpile": "babel ./src --out-dir dist",
    "clean_dist": "rimraf dist",
    "build": "npm-run-all clean_dist transpile",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": ["@babel/preset-env"]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": ["server/*", "public/*"],
    "ignore": ["**/__tests__/**", "*.test.js", "*.spec.js"]
  }
}
