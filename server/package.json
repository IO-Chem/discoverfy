{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/main.bundle.js",
    "dev": "npx concurrently \"npm:build:watch\" \"npm:dev:watch\"",
    "build:watch": "WATCHPACK_POLLING=true NODE_ENV=\"development\" npx webpack -w",
    "build:dev": "NODE_ENV=\"development\" npx webpack",
    "dev:watch": "npx nodemon -L ./dist/main.bundle.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.19",
    "request": "^2.88.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
